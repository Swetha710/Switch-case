#include<stdio.h>

int main()
{
  printf("Enter your choice:1\t2\t3\t4\t5\n");
  int choice;
  scanf("%d", &choice);
  switch(choice){
    case 1: 
           printf("Food item:Pizza\nPrice:Rs 120");
            break;
    case 2: 
            printf("Food item:Makhani Pasta\nPrice:Rs 160");
            break;
    case 3: 
            printf("Food item:Pasta\nPrice:Rs 179");
            break;
    case 4: 
            printf("Food item:Chineese Noodles\nPrice:Rs 150");
            break;
    case 5: 
            printf("Food item:Chicken Lolipop\nPrice:Rs 149");
            break;
    default : printf("invalid choice!");}
}
